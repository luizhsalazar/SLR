<br>
<div class="container-fluid">
  <div class="table">
    <table class="table table-hover">
      <tr>
        <th> Base Digital </th>
        <th> Resultados </th>
      </tr>
      <tr>
        <% Reference.all.each do |reference| %>
            <% if reference.protocol_id == params[:id] %>
              <tr>
                <td>
                  <%= link_to reference.database_name, scidirs_path %>
                </td>
                <td>
                  <%= reference.results %>
                </td>
              </tr>
            <% end %>
        <% end %>
      </tr>
    </table>
  </div>
</div>