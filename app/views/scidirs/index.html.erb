<% @scidirs.each do |scidir| %>

    <br>
    <div class="panel panel-default">
      <div class="panel-heading"> <b><%= link_to scidir.title, scidir.link %> </b> </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-12" align="left">
            <h5>
              <b> Autor: </b> <%= scidir.author %> <br>
              <b> Periódico/Conferência: </b> <%= scidir.pubtitle  %><br>
              <b> Abstract: </b> <p> <%= scidir.abstract %> </p>
            </h5>
          </div>
        </div>
      </div>
    </div>
<% end %>