<br>

<div class="btn-group">
  <%= link_to 'Continuar', protocol_path(params[:id]), class: "btn btn-default" %>
</div>

<br>

<% @ieees.each do |ieee| %>
    <br>
    <div class="panel panel-default">
      <div class="panel-heading"> <b><%= link_to ieee.title, ieee.link  %> </b>
        <%= link_to include_ieee_path(ieee), method: :put, class: 'btn btn-default btn-sm' do %>
            <span class="glyphicon glyphicon-chevron-up"></span>
            Incluir
        <% end %>
        <%= link_to exclude_ieee_path(ieee), method: :put, class: 'btn btn-default btn-sm' do %>
            <span class="glyphicon glyphicon-chevron-down"></span>
            Excluir
        <% end %>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-12" align="left">
            <h5>
              <b> Autor: </b> <%= ieee.author %> <br>
              <b> Periódico/Conferência: </b> <%= ieee.pubtitle  %><br>
              <b> Abstract: </b> <p> <%= ieee.abstract %> </p>
            </h5>
          </div>
        </div>
      </div>
    </div>
<% end %>